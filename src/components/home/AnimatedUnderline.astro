    connectedCallback() {
      const svg = this.querySelector("svg");
      const mask = this.querySelector("mask path");

      if (!mask) return;

      svg?.style.setProperty("visibility", "visible");

      animate(
        mask as SVGPathElement,
        { pathLength: [0, 1] },
        {
          duration: 0.7,
          ease: "circOut",
          delay: this.#delay,
        },
      );
    }
  }

  customElements.define("animated-underline", AnimatedUnderline);
</script>
